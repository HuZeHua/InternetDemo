		</bindings>
		<client>
			<endpoint address="http://localhost:20140/ws.asmx" binding="basicHttpBinding" bindingConfiguration="WSSoap" contract="WS.WSSoap" name="WSSoap" />
		</client>
	</system.serviceModel>

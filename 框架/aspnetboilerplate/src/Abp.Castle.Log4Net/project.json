{
  "version": "1.1.3.0-*",

  "dependencies": {
    "Abp": "1.1.3.0-*",
    "log4net": "2.0.5"
  },

  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Configuration": "",
        "System.Runtime": {
          "type": "build"
        },
        "System.Threading.Tasks": {
          "type": "build"
        }
      }
    }
  },

  "buildOptions": {
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },

  "packOptions": {
    "summary": "ASP.NET Boilerplate is a general-purpose, web focused application framework.",
    "tags": [ "asp.net", "asp.net mvc", "boilerplate", "application framework", "web framework", "framework", "domain driven design", "log4net", "castle" ],
    "owners": [ "Halil Ýbrahim Kalkan" ],
    "iconUrl": "http://www.aspnetboilerplate.com/images/abp_nupkg.png",
    "projectUrl": "http://www.aspnetboilerplate.com/",
    "licenseUrl": "https://github.com/aspnetboilerplate/aspnetboilerplate/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnetboilerplate/aspnetboilerplate"
    },
    "files": {
      "mappings": {
        "lib/net452/": "bin/Release/net452/Abp.Castle.Log4Net.pdb"
      }
    }
  }
}

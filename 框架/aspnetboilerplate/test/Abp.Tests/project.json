{
  "version": "1.0.0-*",
  
  "testRunner": "xunit",

  "dependencies": {
    "Abp": "1.1.3.0-*",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "NSubstitute": "1.10.0",
    "Shouldly": "2.8.2",
    "xunit": "2.2.0-beta4-build3444",
    "xunit.extensibility.execution": "2.2.0-beta4-build3444",
    "xunit.runner.visualstudio": "2.2.0-beta4-build1194"
  },

  "frameworks": {
    "net452": {
      "Microsoft.NETCore.App": "1.0.0-rc2-3002702"
    }
  },

  "buildOptions": {
    "embed": {
      "include": [
        "Localization/Json/JsonSources/*.json",
        "Localization/Json/XmlSources/*.xml",
        "Localization/TestResourceFiles/*.resx",
        "Localization/TestXmlFiles/*.xml",
        "Resources/Embedded/MyResources/js/*.js"
      ]
    }
  }
}
